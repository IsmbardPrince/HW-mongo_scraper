  <div id="index-banner" class="parallax-container">
    <div class="section no-pad-bot">
      <div class="container">
        <br><br>
        <h1 class="header center teal-text text-lighten-2">Mongo-Scraper     Salon Edition</h1>
        <div class="row center">
          <h5 class="header col s12 light">These are the currently saved articles for this Mongo-Scraper edition</h5>
        </div>
        <br><br>

      </div>
    </div>
    <div class="parallax"><img src="/assets/img/background1.jpg" alt="Unsplashed background img 1"></div>
  </div>

  <div class="container">
    <div class="section">

      <!--   Icon Section   -->
      <div class="row">
        <div class="col s12 m12">
          <table class="article-table bordered">
            <tbody>
              {{#each articles}}
                <tr>
                  <td><a class="article-link" href="{{this.link}}" target="_blank">{{this.title}}</a></td>
                  <td><a class="comments-show waves-effect waves-light btn" article-id="{{this._id}}">Show&nbsp;Comments</a></td>
                  <td><a class="comment-add waves-effect waves-light btn" article-id="{{this._id}}">Add&nbsp;Comment</a></td>
                  <td><a class="article-delete waves-effect waves-light btn" article-id="{{this._id}}">Delete&nbsp;Article</a></td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>

    </div>
  </div>

  {{> addcomment}}

  <script type="text/javascript" src="/assets/js/savedarticles.js"></script>
